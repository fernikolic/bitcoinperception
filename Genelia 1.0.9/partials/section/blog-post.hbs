<section class="main-wrapper">
	<div class="container-xxl">
		<div class="row mb-4">
			<div class="col-md-6">
				<h3 class="section-title-m text-uppercase">{{>svg/polygonal}}{{t "Latest"}}</h3>
			</div>
			<div class="col-md-6 text-end"></div>
		</div>

		<div class="row">
			<div class="col-lg-12">
				{{!-- The main content area --}}
				<div class="site-main">
					<div class="row gx-5" id="post-masonry">
						{{#foreach posts}}
							{{!-- The tag below includes the markup for each post - partials/post-content.hbs --}}
							<div class="col-md-6 col-lg-4 post-loop">
								{{> "post-content"}}
							</div>
						{{/foreach}}
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-12 text-center mt-4">
				{{pagination}}
			</div>
		</div>
	</div>
</section>
<!--~./ end main wrapper ~-->